<tr>
  <td><%= pin.description %></td>
  <td class="options">
	  <i id="like<%=pin.id %>" class='like icon-thumbs-up'></i>
	  <i id="dislike<%=pin.id %>" class='dislike icon-thumbs-down'></i>
	  <%= link_to raw("<i class='icon-search'></i>"), pin %>
	  <% if current_user == pin.user %>
		  <%= link_to raw("<i class='icon-pencil'></i>"), edit_pin_path(pin) %>
		  <%= link_to raw("<i class='icon-trash'></i>"), pin, method: :delete, data: { confirm: 'Are you sure?' } %>
		  </td>
		<% else %>
		  </td>
		<% end %>
</tr>